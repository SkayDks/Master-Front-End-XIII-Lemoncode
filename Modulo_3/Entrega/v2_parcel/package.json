{
  "name": "parcel_version",
  "version": "1.0.0",
  "description": "",
  "browserlist": [
    "defaults"
  ],
  "source": "src/index.html",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "rimraf dist && parcel --open",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "run-p -l type-check build:prod",
    "build:prod": "rimraf dist && parcel build",
    "build:pref": "run-p -l type-check bundle-analyzer",
    "bundle-analyzer": "rimraf parcel-bundle-reports && npm run build:prod -- --reporter @parcel/reporter-bundle-analyzer",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "@parcel/transformer-sass": {
    "sassModules": {
      "pattern": "[name]__[local]--[hash]"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.9.3",
    "@parcel/transformer-sass": "^2.9.3",
    "@types/node": "^20.4.5",
    "dotenv": "^16.3.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.9.3",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
